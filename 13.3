//P13.3:string function (strcmp)
#include <stdio.h>
#include <string.h>

void main() 
{
    
    char a[20];
    char b[20];
    printf("enter variable a:");
    scanf("%s",&a);
    printf("enter variable b:");
    scanf("%s",&b);
    if(strcmp(a,b)==0)
    {
        printf("string are equal");
    }
    else
    {
        printf("string are not equal");
    }
    
}
    
