#include <stdio.h>

int main() {
  int i,n;
  printf("enter number:");
  scanf("%d",&n);
  printf("multiplication of %d\n",n);
   
   for (i=1;i<=10;i++)
 {
     printf("%dx%d=%d\n",n,i,n*i);
     
    
 }
    
    
}
 
