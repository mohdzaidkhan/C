#include <stdio.h>

int main() 
{
   int n;
   printf("enter number");
   scanf("%d",&n);
   int i=1;
   while(i<=n)
    {
        printf("hello %d",i);
        printf("\n");
        i++;
    
    }
    
  
    
}    
    
    
    
    
    
    
