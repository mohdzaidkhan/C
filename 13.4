//P13.4 string function (Concatenates)
// Append last variable to first variable
#include <stdio.h>
#include <string.h>

void main() 
{
    
    char a[20];
    char b[20];
    printf("enter variable a:");
    scanf("%s",&a);
    printf("enter variable b:");
    scanf("%s",&b);
    strcat(a,b);
    printf("a:%s\n",a);
  

}
    
