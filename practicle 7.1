#include <stdio.h>

int main() {
int week;
printf("enter week number(1-7)");
switch(week)
{
 case 1:
     printf("monday");
 break;
 case 2:
     printf("tuesday");
 break;
 case 3:
     printf("wednesday");
 break;     
 case 4:
     printf("thursday");
 break;
 case 5:
     printf("friday");
 break;
 case 6:
     printf("saturday");
 break;
 case 7:
     printf("sunday");
 break;
 default:
 printf("invalid input please enter number between 1-7.");
}

}
 
